{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach To Kube",
            "protocol": "inspector",
            "sourceMaps": true,
            "address": "localhost",
            "cwd": "${workspaceRoot}",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/home/app",
            "port": 30209,
            "restart": true,
            "smartStep": true
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 60006,
            "protocol": "inspector",
            "sourceMaps": true,
            "restart": true,
            "cwd": "${workspaceRoot}",
            "outFiles": ["${workspaceRoot}/server/app.js"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Webpack Debug",
            "protocol": "inspector",
            "program": "${workspaceRoot}/src/server/config/server.ts",
            "cwd": "${workspaceRoot}",
            "port": 9208,
            "outFiles": ["${workspaceRoot}/server/server.js"],
            "sourceMaps": true
        }
    ]
}
